<Window x:Class="_4332Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="4332 Project" Height="350" Width="525">
    <Grid>
        <Button x:Name="MukhametshinButton" Content="Информация о пользователе" HorizontalAlignment="Center" VerticalAlignment="Top" Width="200" Height="30" Margin="10,10,0,0" Click="MukhametshinButton_Click"/>

        <Button x:Name="ImportDataButton" Content="Импорт данных из Excel" HorizontalAlignment="Center" VerticalAlignment="Top" Width="200" Height="30" Margin="10,50,0,0" Click="ImportDataButton_Click"/>

        <Button x:Name="ExportDataButton" Content="Экспорт данных в Excel" HorizontalAlignment="Center" VerticalAlignment="Top" Width="200" Height="30" Margin="10,90,0,0" Click="ExportDataButton_Click"/>

        <Button x:Name="ImportJsonDataButton" Content="Импорт данных из JSON" HorizontalAlignment="Center" VerticalAlignment="Top" Width="200" Height="30" Margin="10,130,0,0" Click="ImportJsonDataButton_Click"/>

        <Button x:Name="ExportToWordButton" Content="Экспорт данных в Word" HorizontalAlignment="Center" VerticalAlignment="Top" Width="200" Height="30" Margin="10,170,0,0" Click="ExportToWordButton_Click"/>
    </Grid>
</Window>
